{
  "name": "adit",
  "version": "1.0.3",
  "description": "SSH tunnel - in any way you want it",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel index.js -d dist",
    "watch": "gaze 'npm run build' index.js",
    "watch-test": "mocha --watch",
    "lint": "npm run eslint && npm run jscs",
    "jscs": "jscs index.js test",
    "eslint": "eslint index.js test",
    "test": "npm run lint && mocha",
    "coverage": "istanbul cover _mocha -- test",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls",
    "ci": "npm run lint && npm run coveralls",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "github.com/markelog/adit"
  },
  "keywords": [
    "tunnel",
    "adit",
    "forwarding",
    "reverse",
    "ssh"
  ],
  "files": [
    "dist",
    "license"
  ],
  "author": "Oleg Gaidarenko <markelog@gmail.com>",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/markelog/adit/issues"
  },
  "homepage": "https://github.com/markelog/adit",
  "dependencies": {
    "babel-polyfill": "^6.6.0",
    "ssh-url": "^0.1.5",
    "ssh2": "^0.4.13",
    "vow": "^0.4.12"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.6.0",
    "chai": "^3.3.0",
    "coveralls": "^2.11.8",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^6.0.2",
    "eslint-plugin-react": "^4.1.0",
    "gaze-cli": "^0.2.0",
    "istanbul": "^1.0.0-alpha.2",
    "jscs": "^2.10.0",
    "mocha": "^2.4.5",
    "sinon": "^1.17.1",
    "sinon-chai": "^2.8.0"
  }
}
